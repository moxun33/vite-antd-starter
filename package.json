{
  "name": "rvite-antd-starter",
  "version": "0.1.1",
  "description": " app, built in React.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "prepare": "node scripts/postinstall.ts && husky install",
    "lint": "npm run lint:es && yarn lint:style",
    "lint:es": "eslint src --fix --ext .ts,.tsx ",
    "lint:style": "stylelint src/**/*.less  --fix --custom-syntax postcss-less",
    "format": "prettier --write ",
    "postbuild": "node scripts/version.js",
    "commit": "git pull && git add -A && git-cz && git push ",
    "test": "jest --passWithNoTests"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint:es",
      "npm run test"
    ],
    "*.{css,less}": [
      "npm run format",
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.21.0",
    "axios": "^0.27.2",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "nanoid": "^3.3.4",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight": "^0.14.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^5.2.0",
    "valtio": "^1.6.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/babel__core": "^7.1.15",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^26.0.15",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.6.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-react": "^1.0.5",
    "antd-dayjs-vite-plugin": "^1.1.4",
    "antd-vite-import-plugin": "^1.0.3",
    "babel-jest": "^28.1.2",
    "commitizen": "^4.2.4",
    "cz-git": "^1.3.9",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "fs-extra": "^10.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.0.3",
    "magic-string": "^0.26.2",
    "minimist": "^1.2.5",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "semver": "^7.3.5",
    "standard-version": "^9.5.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "ts-babel": "^6.1.7",
    "ts-jest": "^28.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5",
    "vite": "^2.9.13",
    "vite-plugin-imp": "^2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "resolutions": {
    "@types/react": "17.0.45"
  }
}
